<% lookup_keys.each_with_index do |lookup_key, index| %>
  <% lookup_value = lookup_value_for_key(obj, lookup_key) %>
    <%= render "puppetclasses/single_param", :obj => obj, :lookup_key => lookup_key, :lookup_value => lookup_value,
                                             :puppetclass => puppetclass, :index => index, :rowspan => lookup_keys.size %>
<% end %>
