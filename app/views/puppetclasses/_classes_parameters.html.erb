<table class="table table-condensed" id="inherited_puppetclasses_parameters">
  <thead>
  <tr>
    <th class='span3'>Puppet class</th>
    <th class='span2'>Name</th>
    <th class='span7'>Value</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% klasses = obj.all_puppetclasses.map{|pc| pc if pc.class_params.override.any? || pc.lookup_keys.any?}.compact %>
  <%= render :partial => "puppetclasses/class_parameters", :collection => klasses, :as => :klass,
             :locals => { :type => obj.class.to_s.downcase, :host => obj } %>
  </tbody>
</table>