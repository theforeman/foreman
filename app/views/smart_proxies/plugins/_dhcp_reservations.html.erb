<table id="<%= subnet.network.parameterize %>_reservations" class='<%= table_css_classes %> table-fixed' data-table="inline">
  <thead>
    <tr>
      <th class="col-md-4"><%= _("Hostname") %></th>
      <th class="col-md-2"><%= _("IP Address") %></th>
      <th class="col-md-2"><%= _("MAC") %></th>
    </tr>
  </thead>
  <tbody>
    <% subnet.reservations.each do |record| %>
      <tr>
        <td class="ellipsis"><%= link_to_host subnet, record['hostname'], @smart_proxy %></td>
        <td><%=record['ip'] %></td>
        <td><%= record['mac'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
