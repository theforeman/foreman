<%= autosign_form %>
<table class='<%= table_css_classes %>' data-table="inline">
  <thead>
    <tr>
      <th><%= _("Name") %></th>
      <th><%= _('Actions') %></th>
    </tr>
  </thead>
  <tbody>
    <% autosign.each do |cert| %>
      <tr>
        <td><%= h cert%>
        </td>
        <td>
          <%= action_buttons(display_delete_if_authorized hash_for_smart_proxy_autosign_path(:smart_proxy_id => @proxy, :id => cert).merge(:auth_object => @proxy))%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
